<odoo>
    <record id="view_product_label_wizard_form" model="ir.ui.view">
        <field name="name">product.label.wizard.form</field>
        <field name="model">product.label.wizard</field>
        <field name="arch" type="xml">
            <form string="Product Label Wizard">
                <group>
                    <field name="product_id"/>
                    <field name="lot_id"/>
                    <field name="use_date"/>
                    <field name="quantity"/>
                    <field name="uom_id"/>
                    <field name="ingredients"/>
                    <field name="rdgs"/>
                    <field name="description_pickingout"/>
                    <field name="title"/>
                    <field name="label_type"/>
                </group>
                <footer>
                    <button string="Print" type="object" name="print" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
